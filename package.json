{
  "name": "dns-scanner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "test": "mocha ./test --recursive",
    "docker-build-production": "docker build -f Dockerfile -t dns-scanner:production --build-arg build_env=production .",
    "docker-build-development": "docker build -f Dockerfile -t dns-scanner:development --build-arg build_env=development .",
    "docker-rm": "docker stop dns-scanner && docker rm dns-scanner",
    "docker-run-development": "docker run -d --name dns-scanner dns-scanner:development --loglevel=debug --mode=sequence --from 0.0.0.0 --to=0.0.0.10",
    "docker-run-development-live": "docker run -d --name dns-scanner dns-scanner:development --loglevel=debug --mode=sequence --from 0.0.0.0 --to=0.0.0.10 && docker logs --follow dns-scanner",
    "docker-run-production": "docker run -d --name dns-scanner dns-scanner:production --loglevel=debug --mode=sequence --from 0.0.0.0 --to=0.0.0.10",
    "docker-run-production-live": "docker run -d --name dns-scanner dns-scanner:production --loglevel=debug --mode=sequence --from 0.0.0.0 --to=0.0.0.10 && docker logs --follow dns-scanner"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "core-js": "^3.8.3",
    "dotenv": "^8.2.0",
    "loglevel": "^1.7.1",
    "mongoose": "^5.11.15",
    "regenerator-runtime": "^0.13.7",
    "shelljs": "^0.8.4",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/register": "^7.12.13",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1"
  }
}
